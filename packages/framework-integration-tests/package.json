    "@types/nedb": "^1.8.12",
    "@types/node": "^18.15.3",
    "@types/sinon": "10.0.0",
    "@types/sinon-chai": "3.2.5",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@apollo/client": "3.7.13",
    "cdktf": "^0.15.0",
    "cdktf-cli": "^0.14.3",
    "ink": "^3.0.5",
    "react": "^17.0.0",
    "constructs": "^10.0.0",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "child-process-promise": "^2.2.1",
    "cross-fetch": "3.1.5",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "3.4.0",
    "faker": "5.1.0",
    "graphology-types": "^0.24.0",
    "jsonwebtoken": "9.0.1",
    "jwks-rsa": "3.0.1",
    "mocha": "10.2.0",
    "mocha-skip-if": "0.0.3",
    "nedb": "^1.8.0",
    "nyc": "^15.1.0",
    "prettier": "2.3.0",
    "rimraf": "^5.0.0",
