    "@boostercloud/cli": "workspace:^2.1.0",
    "@boostercloud/framework-provider-aws-infrastructure": "workspace:^2.1.0",
    "@boostercloud/framework-provider-azure-infrastructure": "workspace:^2.1.0",
    "@boostercloud/framework-provider-local-infrastructure": "workspace:^2.1.0",
    "@boostercloud/metadata-booster": "workspace:^2.1.0",
    "@types/aws-lambda": "8.10.48",
