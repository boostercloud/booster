{
  "name": "@boostercloud/framework-core",
  "version": "3.0.0",
  "description": "Library for your Booster apps",
  "author": "Boosterin Labs SLU",
  "homepage": "https://boosterframework.com",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boostercloud/booster.git"
  },
  "scripts": {
    "prepare": "ts-patch install -s",
    "format": "prettier --write --ext '.js,.ts' **/*.ts **/*/*.ts",
    "lint:check": "eslint --ext '.js,.ts' **/*.ts",
    "lint:fix": "eslint --quiet --fix --ext '.js,.ts' **/*.ts",
    "build": "tsc -b tsconfig.json",
    "clean": "rimraf ./dist tsconfig.tsbuildinfo",
    "prepack": "tsc -b tsconfig.json",
    "test:core": "npm run test",
    "test": "cross-env BOOSTER_ENV=test nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\" 2>&1"
  },
  "bugs": {
    "url": "https://github.com/boostercloud/booster/issues"
  },
  "engines": {
    "node": ">=20.0.0 <21.0.0"
  },
  "peerDependencies": {
    "graphql": "^16.6.0"
  },
  "dependencies": {
    "@boostercloud/framework-common-helpers": "workspace:^3.0.0",
    "@boostercloud/framework-types": "workspace:^3.0.0",
    "fp-ts": "^2.11.0",
    "graphql-scalars": "^1.17.0",
    "graphql-subscriptions": "2.0.0",
    "inflected": "2.1.0",
    "iterall": "1.3.0",
    "jsonwebtoken": "9.0.1",
    "jwks-rsa": "3.0.1",
    "reflect-metadata": "0.1.13",
    "tslib": "^2.4.0",
    "validator": "13.7.0",
    "effect": "2.4.17",
    "@effect/platform": "0.48.24",
    "@effect/schema": "0.64.18",
    "@effect/cli": "0.35.26",
    "fast-check": "^3.13.2",
    "@effect/printer": "0.32.2",
    "@effect/typeclass": "0.23.17",
    "@effect/printer-ansi": "0.32.26",
    "@effect/platform-node": "0.45.26",
    "ws": "8.17.1"
  },
  "devDependencies": {
    "@boostercloud/metadata-booster": "workspace:^3.0.0",
    "@boostercloud/eslint-config": "workspace:^3.0.0",
    "@types/chai": "4.2.18",
    "@types/chai-as-promised": "7.1.4",
    "@types/faker": "5.1.5",
    "@types/inflected": "1.1.29",
    "@types/jsonwebtoken": "9.0.1",
    "@types/mocha": "10.0.1",
    "@types/node": "^20.17.17",
    "@types/sinon": "10.0.0",
    "@types/sinon-chai": "3.2.5",
    "@types/validator": "13.1.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "cross-env": "7.0.3",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "3.4.0",
    "faker": "5.1.0",
    "mocha": "10.2.0",
    "mock-jwks": "1.0.3",
    "nock": "11.8.2",
    "nyc": "^15.1.0",
    "prettier": "2.3.0",
    "rimraf": "^5.0.0",
    "sinon": "9.2.3",
    "sinon-chai": "3.5.0",
    "ts-node": "^10.9.1",
    "typescript": "5.1.6",
    "eslint-plugin-unicorn": "~44.0.2",
    "graphql": "^16.6.0",
    "ts-patch": "3.1.2"
  },
  "pnpm": {
    "overrides": {
      "pac-resolver@<5.0.0": ">=5.0.0",
      "underscore@>=1.3.2 <1.12.1": ">=1.13.6",
      "node-fetch@<2.6.7": ">=2.6.7",
      "ws@>=7.0.0 <7.4.6": ">=7.4.6",
      "nanoid@>=3.0.0 <3.1.31": ">=3.1.31",
      "node-fetch@<2.6.1": ">=2.6.1"
    }
  }
}
